    "version": "0.2.0",
    "configurations": [
        {
            "name": "elopage_performance",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define=api_token=API_TOKEN",
                "--dart-define=user=USER@MAIL.COM",
            ],
        },
        {
            "name": "elopage_performance (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--dart-define=api_token=API_TOKEN",
                "--dart-define=user=USER@MAIL.COM",
            ],
        },
        {
            "name": "elopage_performance (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--dart-define=api_token=API_TOKEN",
                "--dart-define=user=USER@MAIL.COM",
            ],
        }
    ]
}